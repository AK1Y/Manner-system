ndex\"\r\n                    ></el-table-column>\r\n                    <el-table-column\r\n                        align=\"center\"\r\n                        prop=\"name\"\r\n                        label=\"类型名称\"\r\n                    ></el-table-column>\r\n                    <el-table-column\r\n                        align=\"center\"\r\n                        prop=\"createTime\"\r\n                        label=\"创建时间\"\r\n                    ></el-table-column>\r\n\t\t\t\t\t<el-table-column\r\n\t\t\t\t\t    align=\"center\"\r\n\t\t\t\t\t    prop=\"updateTime\"\r\n\t\t\t\t\t    label=\"修改时间\"\r\n\t\t\t\t\t></el-table-column>\r\n                    <el-table-column\r\n                        align=\"center\"\r\n                        label=\"操作处理\"\r\n                        fixed=\"right\"\r\n                        width=\"250\"\r\n                    >\r\n                        <template slot-scope=\"scope\">\r\n                            <el-button\r\n                                type=\"primary\"\r\n                                @click=\"showUpdWin(scope.row)\"\r\n                                style=\"font-size: 18px\"\r\n                            >\r\n                                \r\n                                编辑\r\n                            </el-button>\r\n                            <el-button\r\n                                type=\"danger\"\r\n                                @click=\"delInfo(scope.row.id)\"\r\n                                style=\"font-size: 18px\"\r\n                            >\r\n                                \r\n                                删除\r\n                            </el-button>\r\n                        </template>\r\n                    </el-table-column>\r\n                </el-table>\r\n                <el-pagination\r\n                    v-if=\"pageTotal >= 0\"\r\n                    style=\"margin-top: 15px\"\r\n                    @size-change=\"handleSizeChange\"\r\n              